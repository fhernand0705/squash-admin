<div class="coaches-container">
  <div class="row">
    <div class="column">Coaches: 2019 Squash Season</div>
      <!-- add cohort signup button -->
    <div class="column">Testing</div>
  </div>
  <table class="coaches-table">
    <tr>
      <th>Id</th>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
    <% @coach.each do |c| %>
    <tr class="coach-trows">
      <td><%= link_to c.id, c %></td>
      <td><%= c.first_name %></td>
      <td><%= c.last_name %></td>
    </tr>
    <% end %>
  </table>

</div>
